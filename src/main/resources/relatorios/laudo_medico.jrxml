	<parameter name="ID_CONSULTA" class="java.lang.Long"/>
	<queryString language="SQL">
		<![CDATA[SELECT
    c.data AS data_consulta,
    c.horario,
    c.observacoes,
    c.receita,
    c.solicitacao_exames,
    c.pressao_arterial,
    c.peso,
    c.altura,
    c.temperatura,
    c.desfecho,
    p.nome AS nome_paciente,
    p.cpf AS cpf_paciente,
    p.data_nascimento,
    EXTRACT(YEAR FROM AGE(p.data_nascimento)) as idade_paciente,
    m.nome AS nome_medico,
    m.crm
FROM consulta c
INNER JOIN paciente p ON c.codigo_paciente = p.codigo
INNER JOIN medico m ON c.codigo_medico = m.codigo
WHERE c.codigo = $P{ID_CONSULTA}]]>
